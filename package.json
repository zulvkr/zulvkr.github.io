{
  "name": "zulvkr.github.io",
  "version": "1.0.0",
  "description": "Personal website",
  "main": ".eleventy.js",
  "scripts": {
    "test": "DEBUG=Eleventy* npx eleventy --dryrun",
    "dev:pre": "npx tailwindcss -i src/assets/main.tailwind.css -o src/temp/main.css",
    "dev:11ty": "npx eleventy --serve",
    "dev:css": "npx tailwindcss -i src/assets/main.tailwind.css -o src/temp/main.css --watch",
    "dev": "npm run dev:pre && (npm run dev:11ty & npm run dev:css)",
    "clean": "rm -r _site src/temp",
    "build:11ty": "NODE_ENV=production npx eleventy",
    "build:css": "NODE_ENV=production npx tailwindcss -i src/assets/main.tailwind.css -o _site/main.css --minify",
    "build": "npm run build:css & npm run build:11ty",
    "preview": "npm run clean && npm run build && browser-sync _site"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zulvkr/zulvkr.github.io.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zulvkr/zulvkr.github.io/issues"
  },
  "homepage": "https://github.com/zulvkr/zulvkr.github.io#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.1",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.1",
    "cssnano": "^5.0.14",
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "html-minifier-terser": "^5.1.1",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^3.0.8"
  }
}
